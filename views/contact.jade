html
    head
        title='add listing'
    body
        h1 Insert New Listing
        form(method="POST" action="/insertContact")
            p Please enter facebook URL:
            input(type="text" name='facebook')
            p Please enter twitter URL:
            input(type="text" name='twitter')
            p Please enter skype URL:
            input(type="text" name='skype')
            input(type="submit" value="submit")
        a(href="/seeList") click here to see listings
