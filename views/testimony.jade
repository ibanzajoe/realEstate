html
    head
        title='add listing'
    body
        h1 Insert New Listing
        form(method="POST" action="/insertTesti")
            p Please enter picture URL:
            input(type="text" name='url')
            p Please enter testimony:
            input(type="text" name='testimony')
            p Please enter name:
            input(type="text" name='name')
            input(type="submit" value="submit")
        a(href="/seeList") click here to see listings
